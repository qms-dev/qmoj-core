<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--去掉注释-->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--需要配置数据库连接-->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://47.99.148.160:3306/qmoj_dev?characterEncoding=utf8&amp;useSSL=false&amp;serverTimezone=UTC&amp;rewriteBatchedStatements=true"
                        userId="test"
                        password="test123.">
        </jdbcConnection>
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--指定entity生成的位置-->
        <javaModelGenerator targetPackage="team.akina.qmoj.entity" targetProject="./src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!--sql映射文件生成的位置 mapper.xml-->
        <sqlMapGenerator targetPackage="mybatis"  targetProject="./src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!--指定DaoMapper生成的位置 mapper.java-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="team.akina.qmoj.mapper" targetProject="./src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <!--这种方式不会有example，建议选择这种方式-->
        <table tableName="qmoj_question" domainObjectName="QmojQuestion" enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>

        <table tableName="qmoj_question_status" domainObjectName="QmojQuestionStatus" enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>

        <table tableName="qmoj_language" domainObjectName="QmojLanguage" enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>

        <!--只含题目摘要相关字段，用于显示题目列表-->
        <table tableName="qmoj_question" domainObjectName="QmojQuestionSummary" enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
            <ignoreColumn column="question_id"/>
            <ignoreColumn column="content"/>
            <ignoreColumn column="lang_to_valid_playground"/>
            <ignoreColumn column="create_time"/>
            <ignoreColumn column="modify_time"/>
            <ignoreColumn column="platform"/>
            <ignoreColumn column="question_slug"/>
        </table>

        <table tableName="qmoj_user" domainObjectName="QmojUser" enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"/>

    </context>
</generatorConfiguration>

